server {
  server_name   dev-front-tutor.openinfra-kr.org;
  access_log /var/log/nginx/access.log;
  error_log /var/log/nginx/error.log;
  location / {
    root   /home/ubuntu/SugarFree/Front/build;
    index  index.html index.htm;
    try_files $uri /index.html;
  }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/dev-front-tutor.openinfra-kr.org/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/dev-front-tutor.openinfra-kr.org/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    index  index.html index.htm;
    try_files $uri /index.html;
  }

    listen 443 ssl; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}
server {
    if ($host = dev-front-tutor.openinfra-kr.org) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


  listen 80;
  server_name   dev-front-tutor.openinfra-kr.org;
    return 404; # managed by Certbot


}
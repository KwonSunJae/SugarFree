server {

        access_log /var/log/nginx/reverse-access.log;
        error_log /var/log/nginx/reverse_error.log;
    server_name dev-api-tutor.openinfra-kr.org; # managed by Certbot


        location / {
                proxy_pass http://127.0.0.1:5051;
        }

    listen 443 ssl; # managed by Certbot
    ssl_certificate /etc/letsencrypt/live/dev-api-tutor.openinfra-kr.org/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/dev-api-tutor.openinfra-kr.org/privkey.pem; # managed by Certbot
    include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}

server {
    if ($host = dev-api-tutor.openinfra-kr.org) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


        listen 80 ;
        listen [::]:80 ;
    server_name dev-api-tutor.openinfra-kr.org;
    return 404; # managed by Certbot


}